                <ui:Label name="final-score-label" text="Final Score: 0" style="font-size: 32px; color: yellow; margin-bottom: 30px; -unity-text-align: middle-center;" />
                <ui:Button name="restart-button" text="Restart" style="font-size: 24px; padding: 10px 30px;" />
            </ui:VisualElement>
        </ui:VisualElement>
    </ui:VisualElement>
